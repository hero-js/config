{
  "name": "@hero-js/config",
  "version": "1.1.0",
  "description": "A flexible and easy-to-use configuration management library for Node.js applications. It allows you to manage your application's configuration settings, including environment variables, with ease.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist & tsc && cp -r ./dist/src/. ./dist && rm -rf ./dist/src",
    "win-build": "rmdir /s /q .\\dist & tsc && xcopy /s /e /Y .\\dist\\src dist && rmdir /s /q .\\dist\\src",
    "generate-doc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hero-js/config.git"
  },
  "keywords": [
    "env",
    "config",
    "hero",
    "herojs",
    "typescript",
    "framework",
    "dotenv"
  ],
  "author": "Sana Yasfp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hero-js/config/issues"
  },
  "homepage": "https://github.com/hero-js/config#readme",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "directories": {
    "doc": "docs"
  }
}
